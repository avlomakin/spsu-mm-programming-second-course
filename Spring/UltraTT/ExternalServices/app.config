<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.serviceModel>
        <bindings>
          <netTcpBinding>
            <binding name="DefaultNetTcpConfig"/>
            <binding name="NetTcpBinding_IUserService" />
            <binding name="NetTcpBinding_IGameSessionService" />
            <binding name="NetTcpBinding_IMatchMakerService" />
          </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.tcp://localhost:13000/UserService" binding="netTcpBinding"
                bindingConfiguration="DefaultNetTcpConfig" contract="User.IUserService"
                name="NetTcpBinding_IUserService">
            </endpoint>
            <endpoint address="net.tcp://localhost:13001/GameSessionService"
                binding="netTcpBinding" bindingConfiguration="DefaultNetTcpConfig"
                contract="GameSession.IGameSessionService" name="NetTcpBinding_IGameSessionService">
            </endpoint>
            <endpoint address="net.tcp://localhost:13002/MatchMakerService"
                binding="netTcpBinding" bindingConfiguration="DefaultNetTcpConfig"
                contract="MatchMaker.IMatchMakerService" name="NetTcpBinding_IMatchMakerService">
            </endpoint>
        </client>
    </system.serviceModel>
</configuration>